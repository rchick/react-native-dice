{
  "expo": {
    "name": "Eddys Dice",
    "slug": "eddys-dice",
    "entryPoint" : "./App.js",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android" : {
      "package": "com.larkinlion.eddysdice"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.larkinlion.eddysdice"
    }
  }
}